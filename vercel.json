{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "bun install && bun run build",
  "outputDirectory": "api",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}